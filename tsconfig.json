{
  "extends": "@grapp/ts-config/react-native.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "emitDeclarationOnly": false,
    "noEmit": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "noPropertyAccessFromIndexSignature": false
  },
  "include": ["./src/**/*", "./config/**/*"]
}
